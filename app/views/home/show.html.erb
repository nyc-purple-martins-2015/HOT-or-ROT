<% if @photostream == [] %>
  <%= render partial: 'nophotos_stash' %>

<% else %>

<div class="container">
  <div class="wrap">
    <div id="tinderslide">
      <ul>
        <% @photostream.each_with_index do |photo, index| %>
          <li class="pane<%=index%>" data-photoid="<%= photo.id %>">
            <img class="photo" src="<%=photo.image_url%>" />
            <div class="like"></div>
            <div class="dislike"></div>
          </li>
        <% end %>
      </ul>
    </div>
  </div>
</div>

<%= render partial: 'finish_stash' %>

<!-- Don't add random extra whitespace -->


<div id="status"> Start Stashing! </div>

<div class="ps_logos_container">
  <div class="ps_logo_pass"> </div>
  <div class="ps_logo_stash"></div>
</div>

<% end %>
